{
  "name": "rethinkdb-index-tuning",
  "version": "1.0.0",
  "description": "Demonstrates different types of indexes in RethinkDB and how query performance is affected.",
  "main": "index.js",
  "scripts": {
    "seed": "rethinkdb import -f data/posts.json --table index_tuning.posts",
    "setup": "babel-node --presets es2015 ./lib/setup",
    "clean": "babel-node --presets es2015 ./lib/cleanup",
    "start": "babel-node --presets es2015 index"
  },
  "keywords": [
    "rethinkdb",
    "index",
    "query",
    "tuning",
    "benchmark",
    "benchmarkjs",
    "promises"
  ],
  "author": "Brandon Doran <bdoran@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/brandondoran/rethinkdb-index-tuning"
  },
  "license": "MIT",
  "dependencies": {
    "babel-cli": "^6.4.5",
    "babel-preset-es2015": "^6.3.13",
    "babel-register": "^6.4.3",
    "benchmark": "^2.1.0",
    "rethinkdb": "^2.2.1"
  }
}
